<%= form_for @changeset, admin_setting_path(@conn, :update), [class: "cf-form"], fn f -> %>
  <%= sub_inputs f, :options, fn(fo) -> %>
    <%= render "settings.html", Map.put(assigns, :fo, fo) %>
  <% end %>

  <p class="form-actions">
    <%= primary_submit gettext("save changes") %>
    <%= default_button_link gettext("cancel"), to: admin_forum_path(@conn, :index) %>
  </p>
<% end %>
