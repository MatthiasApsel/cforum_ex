<%= form_for @changeset, Path.admin_forum_path(@conn, :create), [class: "cf-form"], fn f -> %>
  <%= render "form.html", Map.put(assigns, :f, f) %>

  <p class="form-actions">
    <%= ViewHelpers.Button.primary_submit gettext("save changes") %>
    <%= ViewHelpers.Button.default_button_link gettext("cancel"), to: Path.admin_forum_path(@conn, :index) %>
  </p>
<% end %>
